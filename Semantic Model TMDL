			partition DimUnitStructure = m
				mode: import
				queryGroup: 'Core Dimension'
				source = ```
						let
						    // --------------------------------------------------------------------------
						    // 1. Source Connection and Table Access
						    // --------------------------------------------------------------------------
						    Source = Sql.Database("mhradb1.database.windows.net", "mhra-platform-UNSOW"),
						    OriginalTable = Source{[Schema="Reporting", Item="DimUnitStructure"]}[Data],
						
						    // --------------------------------------------------------------------------
						    // 2. Initial Mandatory Field Filtering
						    //    - Exclude rows where UnitName3 is null
						    //    - Exclude rows where StructureGroup1 is null or empty text
						    // --------------------------------------------------------------------------
						    #"Filtered Mandatory Fields" = Table.SelectRows(OriginalTable, each
						        [UnitName3] <> null and
						        [StructureGroup1] <> null and
						        [StructureGroup1] <> ""
						    ),
						
						    // --------------------------------------------------------------------------
						    // 3. Define Value Replacements for the 'StructureGroup1' Column
						    // --------------------------------------------------------------------------
						    ReplacementsList = {
						        {"University of South Wales", "USW"},
						        {"PSS Ltd Providing Services to University of South Wales", "PSS Ltd-USW"}, 
						        {"The College Merthyr Tydfil", "CMT"},
						        {"PSS Ltd Providing Services to Royal Welsh College of Music and Drama", "PSS Ltd-RWCMD"},
						        {"Royal Welsh College of Music and Drama", "RWCMD"}
						    },
						
						    // --------------------------------------------------------------------------
						    // 4. Apply 'StructureGroup1' Replacements
						    //    - Iteratively applies each replacement rule from ReplacementsList.
						    // --------------------------------------------------------------------------
						    #"Renamed StructureGroup1 Values" = List.Accumulate(
						        ReplacementsList,           // The list of replacement rules
						        #"Filtered Mandatory Fields", // The initial table to start modifying
						        (currentTable, replacement) => // Function to apply for each rule
						            Table.ReplaceValue(
						                currentTable,           // The table from the previous iteration
						                replacement{0},         // Text to find
						                replacement{1},         // Text to replace with
						                Replacer.ReplaceText,   // The type of replacement
						                {"StructureGroup1"}     // The column to perform replacement in
						            )
						    ),
						
						    // --------------------------------------------------------------------------
						    // 5. Filter for Non-Empty Key Unit/Structure Names
						    //    - Exclude rows where UnitName4 is null or empty text
						    //    - Exclude rows where StructureGroup2 is null or empty text
						    // --------------------------------------------------------------------------
						    #"Filtered UnitName4" = Table.SelectRows(#"Renamed StructureGroup1 Values", each 
						        [UnitName4] <> null and [UnitName4] <> ""
						    ),
						    #"Filtered StructureGroup2" = Table.SelectRows(#"Filtered UnitName4", each 
						        [StructureGroup2] <> null and [StructureGroup2] <> ""
						    ),
						    #"Replaced Value" = Table.ReplaceValue(#"Filtered StructureGroup2","PSS Ltd Providing Services to USW","PSS Ltd-USW",Replacer.ReplaceText,{"StructureGroup1"}),
						    #"Filtered Rows" = Table.SelectRows(#"Replaced Value", each true),
						    #"Filtered Rows1" = Table.SelectRows(#"Filtered Rows", each [StructureGroup1] <> null and [StructureGroup1] <> "")
						in
						    #"Filtered Rows1"
						```

			annotation PBI_ResultType = Table

			annotation PBI_NavigationStepName = Navigation

