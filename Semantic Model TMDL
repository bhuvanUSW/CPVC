			partition FactPersonHeadcountMonthly = m
				mode: import
				source =
						// Load and filter data from Reporting.FactPersonHeadcountMonthly
						let
						    // Step 1: Connect to the database and access the table
						    Source = Sql.Database("mhradb1.database.windows.net", "mhra-platform-UNSOW"),
						    FactPersonHeadcountMonthly = Source{[Schema = "Reporting", Item = "FactPersonHeadcountMonthly"]}[Data],
						
						    // Step 2: Define the date threshold (August 1, 2022)
						    StartDate = #date(2024, 1, 1),
						    StartDateKey = Date.Year(StartDate) * 10000 + Date.Month(StartDate) * 100 + Date.Day(StartDate),
						
						    // Step 3: Filter rows based on FirstDayOfMonthKey
						    FilteredRows = Table.SelectRows(FactPersonHeadcountMonthly, each [FirstDayOfMonthKey] >= StartDateKey)
						in
						    FilteredRows

			annotation PBI_NavigationStepName = Navigation

			annotation PBI_ResultType = Table

