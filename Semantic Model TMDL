			partition DimTransfer = m
				mode: import
				queryGroup: 'Core Dimension'
				source =
						// M Code for: DimTransfer (Further filtered by specific TransferReason for CPVC)
						let
						    // Step 1: Source Connection and Table Access
						    Source_DB = Sql.Database("mhradb1.database.windows.net", "mhra-platform-UNSOW"),
						    DimTransfer_SourceTable = Source_DB{[Schema="Reporting",Item="DimTransfer"]}[Data],
						
						    // Step 2: Filter out rows where TransferReason is null (as per your existing logic)
						    #"Filtered Null TransferReasons" = Table.SelectRows(DimTransfer_SourceTable, each ([TransferReason] <> null)),
						
						    // Step 3: Select Columns relevant for CPVC Transfer/Promotion/Regrade Analysis
						    #"Selected Columns" = Table.SelectColumns(#"Filtered Null TransferReasons", {
						        "TransferKey", "OrganisationKey", "PersonKey", "TransferID", "TransferDate",
						        "TransferType", "TransferReason", "TransferName", "TransferFromPartyID",
						        "TransferFromType", "TransferFromName", "TransferToPartyID", "TransferToType",
						        "TransferToName", "PersonID"
						        // Add any other selected columns you deem necessary from DimTransfer
						    }),
						
						    // Step 4: Change Data Types
						    #"Changed Column Types" = Table.TransformColumnTypes(#"Selected Columns",{
						        {"TransferKey", Int64.Type}, {"OrganisationKey", Int64.Type}, {"PersonKey", Int64.Type},
						        {"TransferID", type text}, {"TransferDate", type date}, {"TransferType", type text},
						        {"TransferReason", type text}, {"TransferName", type text}, {"TransferFromPartyID", type text},
						        {"TransferFromType", type text}, {"TransferFromName", type text}, {"TransferToPartyID", type text},
						        {"TransferToType", type text}, {"TransferToName", type text}, {"PersonID", type text}
						    }),
						
						    // Step 5: Filter for CPVC-Relevant Transfer Reasons (Focus on Promotions/Regrades)
						    // These are reasons most likely to indicate upward movement or significant role changes.
						    // You can adjust this list based on your precise needs.
						    CPVC_Relevant_Transfer_Reasons = {
						        "Transfer - Promotion",
						        "Transfer - HERA",
						        "Transfer - Through internal application",
						        "Transfer - Role evaluation"
						        // Consider adding "Transfer - Acting-up" or "Transfer - Secondment" if relevant for your CPVC focus
						    },
						    #"Filtered for CPVC Reasons" = Table.SelectRows(#"Changed Column Types", each
						        // We also want to ensure the TransferType is 'Person' as these are person-centric changes
						        [TransferType] = "Person" and
						        List.Contains(CPVC_Relevant_Transfer_Reasons, [TransferReason], Comparer.OrdinalIgnoreCase)
						    ),
						    #"Filtered Rows" = Table.SelectRows(#"Filtered for CPVC Reasons", each ([TransferReason] = "Transfer - HERA"))
						in
						    #"Filtered Rows"

			annotation PBI_ResultType = Table

			annotation PBI_NavigationStepName = Navigation

