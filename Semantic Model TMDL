			partition DimPayroll = m
				mode: import
				queryGroup: 'Core Dimension'
				source =
						// M Code for: DimPayroll (CPVC Focused - Ensuring all relevant reward/pension elements are included)
						let
						    // Step 1: Source Connection and Table Access
						    Source_DB = Sql.Database("mhradb1.database.windows.net", "mhra-platform-UNSOW"),
						    DimPayroll_SourceTable = Source_DB{[Schema="Reporting",Item="DimPayroll"]}[Data],
						
						    // Step 2: Select Columns relevant for CPVC Analysis
						    #"Selected CPVC Columns" = Table.SelectColumns(DimPayroll_SourceTable, {
						        "PayrollKey",
						        // "OrganisationKey", // Removed as DimOrganisation is removed, and this key in DimPayroll might be redundant if not used elsewhere
						        "ElementID",
						        "PayrollName",
						        "PayrollCategory", // e.g., "Salary", "Pension", "Bonus", "Expenses", "Other"
						        "ElementCode",
						        "ElementName",     // Key for Market Supplements, Responsibility Allowances
						        "ElementType",     // Payment, Deduction, Benefit - Important for context
						        "ElementFixedType",// Key for Pension Contributions (PEN_EE, PEN_ER etc.)
						        "ElementGroup",
						        "AbsencePayElementGroups" // Used for filtering if appropriate
						    }),
						
						    // Step 3: Ensure Correct Data Types for selected columns
						    #"Changed Column Types" = Table.TransformColumnTypes(#"Selected CPVC Columns",{
						        {"PayrollKey", Int64.Type},
						        // {"OrganisationKey", Int64.Type}, // Only if kept in step 2
						        {"ElementID", type text},
						        {"PayrollName", type text},
						        {"PayrollCategory", type text},
						        {"ElementCode", type text},
						        {"ElementName", type text},
						        {"ElementType", type text},
						        {"ElementFixedType", type text},
						        {"ElementGroup", type text},
						        {"AbsencePayElementGroups", type text}
						    }),
						
						    // Step 4: Filter based on AbsencePayElementGroups
						    // This filter is kept from your last version.
						    // CRITICAL: Verify that 'AbsencePayElementGroups = "Other"' (or any other values you add here)
						    // INCLUDES all elements for Market Supplements, Responsibility Allowances, AND Pension Contributions.
						    // If pension contributions (EE Deductions, ER Benefits) fall under a different AbsencePayElementGroups,
						    // this filter needs to be adjusted, or pension analysis might fail.
						    #"Filtered by AbsencePayElementGroups" = Table.SelectRows(#"Changed Column Types", each true)
						
						    // If the above filter is too restrictive for pensions, an alternative is to remove it
						    // and rely on DAX to pick specific elements. For now, retaining your preference.
						in
						    #"Filtered by AbsencePayElementGroups"

			annotation PBI_NavigationStepName = Navigation

			annotation PBI_ResultType = Table

