			column 'Is HPL'
				dataType: boolean
				formatString: """TRUE"";""TRUE"";""FALSE"""
				lineageTag: 7fe5f5a7-395a-4426-8328-dca2d642d999
				summarizeBy: none
				sourceColumn: Is HPL

				annotation SummarizationSetBy = Automatic

			partition DimPosition = m
				mode: import
				queryGroup: 'Core Dimension'
				source =
						// M Code for: DimPosition_BaseWithHPLFlag
						let
						    // --------------------------------------------------------------------------
						    // 1. Source Connection and Table Access
						    // --------------------------------------------------------------------------
						    Source = Sql.Database("mhradb1.database.windows.net", "mhra-platform-UNSOW"),
						    DimPositionTable = Source{[Schema="Reporting", Item="DimPosition"]}[Data],
						
						    // --------------------------------------------------------------------------
						    // 2. Define Parameters and Thresholds
						    // --------------------------------------------------------------------------
						    CurrentDateTime = DateTime.LocalNow(),
						    StartDateThreshold = #datetime(2024, 1, 1, 0, 0, 0), // August 1st, 2022
						
						    // --------------------------------------------------------------------------
						    // 3. Define Lists for Initial Filtering
						    // --------------------------------------------------------------------------
						    ValidTypes = {
						        "Permanent", "Fixed Term - Project / Task", "Fixed Term - Absence Cover",
						        "Fixed Term cover for Secondment / Acting up", "Perm/Fixed"
						    }, // [cite: 63]
						    ValidBasis_Source = {"Full time", "Part time", "Hourly Paid"}, // Original values before renaming [cite: 64]
						
						    // --------------------------------------------------------------------------
						    // 4. Initial Filtering of Positions
						    //    - Based on Type, Basis, Dates, and excluding 'External Examiner'
						    // --------------------------------------------------------------------------
						    InitialFilteredData = Table.SelectRows(DimPositionTable, each
						        List.Contains(ValidTypes, [Type]) and
						        List.Contains(ValidBasis_Source, [Basis]) and // Use original Basis values for this initial filter
						        [FromDate] >= StartDateThreshold and
						        [ToDate] > CurrentDateTime and // Position is currently active [cite: 65]
						        [Category] <> "External Examiner" // [cite: 65]
						    ),
						
						    // --------------------------------------------------------------------------
						    // 5. Rename 'Category' Values
						    //    (This entire section is from your TMDL for DimPosition) [cite: 65, 66, 67, 68]
						    // --------------------------------------------------------------------------
						    AddRenamedCategory = Table.AddColumn(InitialFilteredData, "_NewCategory", each
						        if [Category] = "Research Staff" then "Research"
						        else if [Category] = "Academic Staff" then "Academic"
						        else if [Category] = "Hourly Paid - Academic" then "Hourly Academic"
						        else if [Category] = "Technical Staff" then "Technical"
						        else if [Category] = "Non Academic Support Staff" then "Support"
						        else if [Category] = "Hourly Paid - Tutor" then "Hourly Tutor"
						        else if [Category] = "Hourly Paid - Technical/Demonstrators" then "Hourly Technical"
						        else if [Category] = "Caretaking Cleaning and Catering" then "Catering"
						        else if [Category] = "Buildings and Maintenance Staff" then "Maintenance"
						        else if [Category] = "Hourly Paid - Non Academic" then "Hourly Non-Academic"
						        else [Category] // Keep original value if no specific rule applies
						    ),
						    ReplaceCategoryColumn = Table.SelectColumns(AddRenamedCategory, Table.ColumnNames(InitialFilteredData) & {"_NewCategory"}),
						    FinalCategoryRename = Table.RenameColumns(
						        Table.RemoveColumns(ReplaceCategoryColumn, {"Category"}),
						        {{"_NewCategory", "Category"}} // New column name is "Category"
						    ),
						
						    // --------------------------------------------------------------------------
						    // 6. Rename 'Basis' Values
						    //    (This entire section is from your TMDL for DimPosition) [cite: 68, 69]
						    // --------------------------------------------------------------------------
						    AddRenamedBasis = Table.AddColumn(FinalCategoryRename, "_NewBasis", each
						        if [Basis] = "Full time" then "Full-time"
						        else if [Basis] = "Part time" then "Part-time"
						        else if [Basis] = "Hourly Paid" then "Hourly" // Renaming "Hourly Paid" to "Hourly"
						        else [Basis] // Keep original value
						    ),
						    ReplaceBasisColumn = Table.SelectColumns(AddRenamedBasis, Table.ColumnNames(FinalCategoryRename) & {"_NewBasis"}),
						    FinalBasisRename = Table.RenameColumns(
						        Table.RemoveColumns(ReplaceBasisColumn, {"Basis"}),
						        {{"_NewBasis", "Basis"}} // New column name is "Basis"
						    ),
						
						    // --------------------------------------------------------------------------
						    // 7. Add 'StaffGroup' Column based on the renamed 'Category'
						    //    (This section is from your TMDL for DimPosition) [cite: 69, 70]
						    // --------------------------------------------------------------------------
						    AddStaffGroup = Table.AddColumn(FinalBasisRename, "StaffGroup", each
						        if List.Contains({"Research", "Academic", "Hourly Academic", "Hourly Tutor"}, [Category]) then // Uses renamed Category
						            "Academic Staff"
						        else
						            "Professional Staff"
						    ),
						
						    // --------------------------------------------------------------------------
						    // 8. Rename 'Type' Values
						    //    (This entire section is from your TMDL for DimPosition) [cite: 70, 71, 72]
						    // --------------------------------------------------------------------------
						    AddRenamedType = Table.AddColumn(AddStaffGroup, "_NewType", each
						        if [Type] = "Fixed Term - Absence Cover" then "Fixed - Absence"
						        else if [Type] = "Fixed Term - Project / Task" then "Fixed - Project"
						        else if [Type] = "Fixed Term cover for Secondment / Acting up" then "Fixed - Secondment"
						        else [Type] // Keep original value
						    ),
						    ReplaceTypeColumn = Table.SelectColumns(AddRenamedType, Table.ColumnNames(AddStaffGroup) & {"_NewType"}),
						    FinalTypeRename_Alias = Table.RenameColumns( // Renamed this step to avoid conflict if you reuse step names
						        Table.RemoveColumns(ReplaceTypeColumn, {"Type"}),
						        {{"_NewType", "Type"}} // New column name is "Type"
						    ),
						
						    // --------------------------------------------------------------------------
						    // **** NEW STEP: Add 'Is HPL' column ****
						    //      HPL Definition: Basis = "Hourly" AND StaffGroup = "Academic Staff"
						    // --------------------------------------------------------------------------
						    #"Added Is HPL Column" = Table.AddColumn(FinalTypeRename_Alias, "Is HPL",
						        each ([Basis] = "Hourly" and [StaffGroup] = "Academic Staff"), type logical),
						    #"Filtered Rows" = Table.SelectRows(#"Added Is HPL Column", each [GradeDescription] <> null and [GradeDescription] <> "")
						        // This uses the [Basis] column which would have been renamed to "Hourly" in step 6 if originally "Hourly Paid"
						in
						    #"Filtered Rows"

			annotation PBI_ResultType = Table

			annotation PBI_NavigationStepName = Navigation

