			partition DimDate = m
				mode: import
				queryGroup: 'Core Dimension'
				source = ```
						// M Code for: DimDate (Comprehensive with Standard, Financial, and Custom Academic Calendar)
						let
						    // Step 1: Generate Date Range
						    StartDate = #date(2023, 8, 1),    // Start of Academic Year 2023-2024
						    EndDate = #date(2026, 7, 31),      // End of Academic Year 2025-2026 (covers AY 2025 Q1, Q2, Q3, Q4)
						    DateList = List.Dates(StartDate, Duration.Days(EndDate - StartDate) + 1, #duration(1,0,0,0)),
						    
						    // Step 2: Convert to Table and Set Initial Type
						    DateTableFromList = Table.FromList(DateList, Splitter.SplitByNothing(), {"FullDate"}, null, ExtraValues.Error),
						    TypedFullDate = Table.TransformColumnTypes(DateTableFromList, {{"FullDate", type date}}),
						    
						    // Step 3: Add Standard Calendar Columns 
						    // (Assuming your iTrent DimDate source might not be used if generating from scratch,
						    // or these are added to an existing DimDate source. The user provided these steps.)
						    AddedDateKey = Table.AddColumn(TypedFullDate, "DateKey", each 
						        Date.Year([FullDate]) * 10000 + Date.Month([FullDate]) * 100 + Date.Day([FullDate]), Int64.Type),
						    AddedDayName = Table.AddColumn(AddedDateKey, "DayName", each 
						        Date.DayOfWeekName([FullDate]), type text),
						    AddedDayNameShort = Table.AddColumn(AddedDayName, "DayNameShort", each 
						        Text.Start([DayName], 3), type text), // Reference corrected to [DayName]
						    AddedDayNumberOfWeek = Table.AddColumn(AddedDayNameShort, "DayNumberOfWeek", each 
						        Date.DayOfWeek([FullDate], Day.Monday) + 1, Int64.Type), // Monday is 1
						    AddedDayNumberOfMonth = Table.AddColumn(AddedDayNumberOfWeek, "DayNumberOfMonth", each 
						        Date.Day([FullDate]), Int64.Type),
						    AddedDayNumberOfYear = Table.AddColumn(AddedDayNumberOfMonth, "DayNumberOfYear", each 
						        Date.DayOfYear([FullDate]), Int64.Type),
						    AddedWeekNumberOfYear = Table.AddColumn(AddedDayNumberOfYear, "WeekNumberOfYear", each 
						        Date.WeekOfYear([FullDate], Day.Monday), Int64.Type), // Week starts Monday
						    AddedCalendarMonthName = Table.AddColumn(AddedWeekNumberOfYear, "CalendarMonthName", each 
						        Date.MonthName([FullDate]), type text),
						    AddedCalendarMonthNameShort = Table.AddColumn(AddedCalendarMonthName, "CalendarMonthNameShort", each 
						        Text.Start([CalendarMonthName], 3), type text), // Reference corrected to [CalendarMonthName]
						    AddedCalendarMonthNumber = Table.AddColumn(AddedCalendarMonthNameShort, "CalendarMonthNumber", each 
						        Date.Month([FullDate]), Int64.Type),
						    AddedCalendarQuarterName = Table.AddColumn(AddedCalendarMonthNumber, "CalendarQuarterName", each 
						        "Q" & Text.From(Date.QuarterOfYear([FullDate])), type text),
						    AddedCalendarQuarterNumber = Table.AddColumn(AddedCalendarQuarterName, "CalendarQuarterNumber", each 
						        Date.QuarterOfYear([FullDate]), Int64.Type),
						    AddedCalendarYearNumber = Table.AddColumn(AddedCalendarQuarterNumber, "CalendarYearNumber", each 
						        Date.Year([FullDate]), Int64.Type),
						    AddedIsWeekend = Table.AddColumn(AddedCalendarYearNumber, "IsWeekend", each 
						        Date.DayOfWeek([FullDate], Day.Monday) >= 5, type logical), // Saturday (5) or Sunday (6)
						
						    // Step 4: Add Financial Year Columns (April to March)
						    AddedFinancialYear = Table.AddColumn(AddedIsWeekend, "FinancialYearNumber", each 
						        if Date.Month([FullDate]) >= 4 
						        then Date.Year([FullDate]) 
						        else Date.Year([FullDate]) - 1, Int64.Type),
						    AddedFinancialMonthNumber = Table.AddColumn(AddedFinancialYear, "FinancialMonthNumber", each
						        let calendarMonth = Date.Month([FullDate])
						        in if calendarMonth >= 4 then calendarMonth - 3 
						           else calendarMonth + 9, Int64.Type),
						    AddedFinancialQuarterNumber = Table.AddColumn(AddedFinancialMonthNumber, "FinancialQuarterNumber", each
						        Number.RoundUp([FinancialMonthNumber] / 3), Int64.Type),
						
						    // Step 5: Add Custom Academic Year (e.g., "2023-2024" for Aug 2023 - Jul 2024)
						    AddedCustomAcademicYear = Table.AddColumn(AddedFinancialQuarterNumber, "AcademicYear", each 
						        let
						            currentYear = Date.Year([FullDate]),
						            currentMonth = Date.Month([FullDate])
						        in
						            if currentMonth >= 8 
						            then Text.From(currentYear) & "-" & Text.From(currentYear + 1)
						            else Text.From(currentYear - 1) & "-" & Text.From(currentYear),
						        type text),
						
						    // Step 6: Add Custom Academic Quarter Number (Q1: Aug-Oct, Q2: Nov-Jan, etc.)
						    AddedCustomAcademicQuarterNumber = Table.AddColumn(AddedCustomAcademicYear, "AcademicQuarterNumber", each
						        let
						            month = Date.Month([FullDate])
						        in
						            if month >= 8 and month <= 10 then 1     // Aug-Oct = Q1
						            else if month = 11 or month = 12 or month = 1 then 2 // Nov, Dec, Jan = Q2  -- Corrected to include month 12
						            else if month >= 2 and month <= 4 then 3 // Feb-Apr = Q3
						            else 4,                                  // May-Jul = Q4
						        Int64.Type),
						
						    // Step 7: Add Custom Academic Quarter Name
						    AddedCustomAcademicQuarterName = Table.AddColumn(AddedCustomAcademicQuarterNumber, "AcademicQuarterName", each
						        "AQ" & Text.From([AcademicQuarterNumber]), type text), // Using "AQ" for clarity
						
						    // Step 8: Add Combined Academic Year-Quarter Name
						    AddedCustomAcademicYearQuarterName = Table.AddColumn(AddedCustomAcademicQuarterName, "AcademicYearQuarterName", each
						        [AcademicYear] & " " & [AcademicQuarterName], type text), // e.g., "2023-2024 AQ1"
						
						    // Step 9: Add Custom Academic Month Number (Aug=1, Sep=2, ..., Jul=12)
						    AddedCustomAcademicMonthNumber = Table.AddColumn(AddedCustomAcademicYearQuarterName, "AcademicMonthNumber", each
						        let calendarMonth = Date.Month([FullDate])
						        in if calendarMonth >= 8 
						            then calendarMonth - 7 
						            else calendarMonth + 5, Int64.Type),
						            
						    // Step 10: Add Custom Academic Month Name
						    AddedCustomAcademicMonthName = Table.AddColumn(AddedCustomAcademicMonthNumber, "AcademicMonthName", each
						        Date.ToText([FullDate], "MMMM", "en-GB"), // Uses FullDate to get correct month name
						        type text),
						
						    // Step 11: Add Custom Academic Quarter Date Ranges (Textual Description)
						    AddedCustomAcademicQuarterMonths = Table.AddColumn(AddedCustomAcademicMonthName, "AcademicQuarterMonths", each
						        let
						            acadYearText = [AcademicYear],
						            quarter = [AcademicQuarterNumber],
						            startYear = Number.FromText(Text.Start(acadYearText, 4)),
						            endYear = Number.FromText(Text.End(acadYearText, 4))
						        in
						            if quarter = 1 then "Aug " & Text.From(startYear) & " - Oct " & Text.From(startYear)
						            else if quarter = 2 then "Nov " & Text.From(startYear) & " - Jan " & Text.From(endYear)
						            else if quarter = 3 then "Feb " & Text.From(endYear) & " - Apr " & Text.From(endYear)
						            else "May " & Text.From(endYear) & " - Jul " & Text.From(endYear),
						        type text),
						    
						    // Step 12: Add Custom Academic Semester
						    AddedCustomAcademicSemester = Table.AddColumn(AddedCustomAcademicQuarterMonths, "AcademicSemester", each
						        let quarter = [AcademicQuarterNumber]
						        in
						            if quarter = 1 then "Autumn"        // Aug-Oct
						            else if quarter = 2 then "Winter"   // Nov-Jan
						            else if quarter = 3 then "Spring"   // Feb-Apr
						            else "Summer",                      // May-Jul
						        type text),
						
						    // Step 13: Add Custom Academic Quarter Start and End Dates
						    AddedCustomAcademicQuarterStartDate = Table.AddColumn(AddedCustomAcademicSemester, "AcademicQuarterStartDate", each
						        let
						            acadYearText = [AcademicYear],
						            quarter = [AcademicQuarterNumber],
						            startYear = Number.FromText(Text.Start(acadYearText, 4))
						        in
						            if quarter = 1 then #date(startYear, 8, 1)
						            else if quarter = 2 then #date(startYear, 11, 1)
						            else if quarter = 3 then #date(startYear + 1, 2, 1)
						            else #date(startYear + 1, 5, 1),
						        type date),
						    
						    AddedCustomAcademicQuarterEndDate = Table.AddColumn(AddedCustomAcademicQuarterStartDate, "AcademicQuarterEndDate", each
						        let
						            acadYearText = [AcademicYear],
						            quarter = [AcademicQuarterNumber],
						            startYear = Number.FromText(Text.Start(acadYearText, 4)),
						            endYear = Number.FromText(Text.End(acadYearText, 4))
						        in
						            if quarter = 1 then #date(startYear, 10, 31)
						            else if quarter = 2 then #date(endYear, 1, 31)
						            else if quarter = 3 then #date(endYear, 4, 30)
						            else #date(endYear, 7, 31),
						        type date),
						
						    // Step 14: Add Custom Academic Week Number (within Academic Quarter)
						    AddedCustomAcademicWeek = Table.AddColumn(AddedCustomAcademicQuarterEndDate, "AcademicWeek", each
						        let
						            quarterStartDate = [AcademicQuarterStartDate],
						            // weekNumber = Number.RoundUp((Duration.Days([FullDate] - quarterStartDate) +1) / 7) // +1 to make first day week 1
						            // More robust week numbering starting from 1 for the first day of the quarter
						            daysIntoQuarter = Duration.Days([FullDate] - quarterStartDate),
						            weekNumber = Number.IntegerDivide(daysIntoQuarter, 7) + 1
						        in
						            weekNumber, Int64.Type),
						
						    // Step 15: Add Year-Month (YYYY-MM for sorting/grouping)
						    AddedStandardYearMonth = Table.AddColumn(AddedCustomAcademicWeek, "YearMonth", each
						        Text.From(Date.Year([FullDate])) & "-" & Text.PadStart(Text.From(Date.Month([FullDate])), 2, "0"), type text),
						
						    // Step 16: Add Month-Year (MMM YYYY for display)
						    AddedStandardMonthYear = Table.AddColumn(AddedStandardYearMonth, "MonthYear", each
						        Text.Start(Date.MonthName([FullDate]), 3) & " " & Text.From(Date.Year([FullDate])), type text),
						
						    // Step 17: Add Academic Month-Year (MMM YYYY using Academic Year context)
						    AddedCustomAcademicMonthYear = Table.AddColumn(AddedStandardMonthYear, "AcademicMonthYear", each
						        let
						            acadMonthNameShort = Text.Start([AcademicMonthName],3),
						            calYearForAcadMonth = 
						                if [AcademicMonthNumber] >= 6 and [AcademicMonthNumber] <=12 then // Jan to Jul (Academic Months 6-12)
						                    Number.FromText(Text.End([AcademicYear],4)) // Use the END year of "YYYY-YYYY"
						                else // Aug to Dec (Academic Months 1-5)
						                    Number.FromText(Text.Start([AcademicYear],4)) // Use the START year of "YYYY-YYYY"
						        in
						            acadMonthNameShort & " " & Text.From(calYearForAcadMonth), type text),
						
						    // Step 18: Final Column Selection and Ordering
						    // This lists all columns created. If your source DimDateTable had other columns you want to keep, add them here.
						    FinalTableSelection = Table.SelectColumns(AddedCustomAcademicMonthYear, {
						        // Primary Keys & Base Date
						        "DateKey", "FullDate",
						        // Standard Calendar Columns
						        "DayName", "DayNameShort", "DayNumberOfWeek", "DayNumberOfMonth", "DayNumberOfYear",
						        "WeekNumberOfYear", "CalendarMonthName", "CalendarMonthNameShort", "CalendarMonthNumber",
						        "CalendarQuarterName", "CalendarQuarterNumber", "CalendarYearNumber", "IsWeekend",
						        "YearMonth", "MonthYear",
						        // Financial Calendar Columns
						        "FinancialYearNumber", "FinancialMonthNumber", "FinancialQuarterNumber",
						        // Academic Calendar Columns
						        "AcademicYear", "AcademicQuarterNumber", "AcademicQuarterName", "AcademicYearQuarterName",
						        "AcademicMonthNumber", "AcademicMonthName", "AcademicQuarterMonths", "AcademicSemester",
						        "AcademicQuarterStartDate", "AcademicQuarterEndDate", "AcademicWeek", "AcademicMonthYear"
						    }),
						    #"Filtered Rows" = Table.SelectRows(FinalTableSelection, each true)
						    // You can add a Table.ReorderColumns step here if you want a specific final order.
						
						in
						    #"Filtered Rows"
						```

			annotation PBI_ResultType = Table

			annotation PBI_NavigationStepName = Navigation

