			column 'Leaver Reporting Category'
				dataType: string
				lineageTag: a03b5660-516f-4278-97bf-67800b8cc124
				summarizeBy: none
				sourceColumn: Leaver Reporting Category

				annotation SummarizationSetBy = Automatic

			partition DimPerson = m
				mode: import
				queryGroup: 'Core Dimension'
				source =
						let
						    // Step 1: Connect to the Azure SQL Database and get the DimPerson table
						    Source = Sql.Database("mhradb1.database.windows.net", "mhra-platform-UNSOW"),
						    Reporting_DimPerson = Source{[Schema="Reporting",Item="DimPerson"]}[Data],
						
						    // Step 2: Your existing filter to remove rows with null or empty LeavingReasonCategory
						    // This is good to keep as it ensures we only process rows with a base leaving category.
						    #"Filtered Rows for Leaving Category" = Table.SelectRows(Reporting_DimPerson, each [LeavingReasonCategory] <> null and [LeavingReasonCategory] <> ""),
						
						    // Step 3: Add the new 'Leaver Reporting Category' column
						    // This logic is based on our detailed discussion and your confirmation
						    // on how to handle Redundancy, Voluntary Schemes, etc.
						    #"Added Leaver Reporting Category" = Table.AddColumn(#"Filtered Rows for Leaving Category", "Leaver Reporting Category", each
						        if [LeavingReason] <> null and Text.Contains([LeavingReason], "Redundancy", Comparer.OrdinalIgnoreCase) then "Redundancy"
						        else if [LeavingReason] = "Voluntary Redundancy" then "Redundancy" // More explicit match for Voluntary Redundancy
						        else if [LeavingReason] = "VES ER Business Efficiency 2024" then "Voluntary Scheme/Severance"
						        else if [LeavingReason] = "Voluntary Exit Scheme 2024" then "Voluntary Scheme/Severance"
						        else if [LeavingReason] = "Voluntary Severance" then "Voluntary Scheme/Severance"
						        else if [LeavingReason] <> null and Text.StartsWith([LeavingReason], "Settlement (mutual) agreement", Comparer.OrdinalIgnoreCase) then "Settlement Agreement"
						        else if [LeavingReasonCategory] = "Resignation" then "Resignation"
						        else if [LeavingReasonCategory] = "Retirement" then "Retirement" // Catches main retirements; redundancy-retirements already tagged as Redundancy above
						        else if [LeavingReasonCategory] = "Dismissal" then "Dismissal (Other)" // Catches other dismissals; redundancy-dismissals tagged as Redundancy above
						        else if [LeavingReasonCategory] = "Death" or ([LeavingReason] <> null and Text.Contains([LeavingReason], "Death in Service", Comparer.OrdinalIgnoreCase)) then "Death"
						        else if [LeavingReasonCategory] = "Transfer" then "Transfer"
						        else if [LeavingReasonCategory] = "Voluntary" then "Other Voluntary" // Catches other 'Voluntary' category types not specified above (e.g., if "Settlement" wasn't caught by StartsWith)
						        else if [LeavingReason] <> null and [LeavingReason] <> "" then "Other Specified Reason" // For records that passed the initial filter but don't match above
						        else "Undefined", // Should be minimal due to your initial filter on LeavingReasonCategory
						        type text
						    ),
						    #"Filtered Rows" = Table.SelectRows(#"Added Leaver Reporting Category", each [Generation] <> null and [Generation] <> ""),
						    #"Filtered Rows1" = Table.SelectRows(#"Filtered Rows", each ([Gender] = "Female" or [Gender] = "Male"))
						in
						    #"Filtered Rows1"

			annotation PBI_NavigationStepName = Navigation

			annotation PBI_ResultType = Table

		table 'Measures Demo'
			lineageTag: 52eecf38-2935-46f1-b72b-4ae50747a67b

