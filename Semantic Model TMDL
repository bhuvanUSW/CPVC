			partition DimAbsence = m
				mode: import
				queryGroup: 'Core Dimension'
				source = ```
						// M Code for: DimAbsence (Focused ONLY on Sickness Records for CPVC)
						let
						    // Step 1: Source Connection and Table Access
						    Source_DB = Sql.Database("mhradb1.database.windows.net", "mhra-platform-UNSOW"),
						    DimAbsence_SourceTable = Source_DB{[Schema="Reporting",Item="DimAbsence"]}[Data],
						
						    // Step 2: Select Columns relevant for Sickness Analysis
						    #"Selected Columns" = Table.SelectColumns(DimAbsence_SourceTable, {
						        "AbsenceKey",
						        "PositionID",
						        "OrganisationKey",
						        "AbsenceGroup",             // Still useful to select to verify the filter, or if a specific "SICK" subtype existed
						        "AbsenceType",
						        "AbsenceReason",
						        "AbsenceTerm",
						        "TotalDaysLost",
						        "Authorised",               // We decided this wasn't needed for S/M/L sickness categories but keeping for now
						        "AbsenceStartDate",
						        "AbsenceEndDate"
						    }),
						
						    // Step 3: Change Data Types
						    #"Changed Column Types" = Table.TransformColumnTypes(#"Selected Columns",{
						        {"AbsenceKey", Int64.Type},
						        {"OrganisationKey", Int64.Type},
						        {"PositionID", type text},
						        {"AbsenceGroup", type text},
						        {"AbsenceType", type text},
						        {"AbsenceReason", type text},
						        {"AbsenceTerm", type text},
						        {"TotalDaysLost", type number},
						        {"Authorised", type text},
						        {"AbsenceStartDate", type datetime},
						        {"AbsenceEndDate", type datetime}
						    }),
						
						    // Step 4: Add the 'Sickness Reporting Category' column
						    // This logic will now only effectively run on rows that are SICK,
						    // but the "else 'Not Sickness'" part is harmless as those rows will be filtered out next.
						    #"Added Sickness Reporting Category" = Table.AddColumn(#"Changed Column Types", "Sickness Reporting Category", each
						        if [AbsenceGroup] = "SICK" then // This 'if' is still good for clarity within the custom column logic
						            if [TotalDaysLost] > 20 then "Sickness - Long-term (>20 days)"
						            else if [TotalDaysLost] >= 7 and [TotalDaysLost] <= 20 then "Sickness - Medium-term (Medical Intervention)"
						            else if [TotalDaysLost] < 7 and [TotalDaysLost] > 0 then "Sickness - Short-term"
						            else "Sickness - Uncategorized"
						        else
						            "Not Sickness", 
						        type text
						    ),
						
						    // Step 5: Filter the ENTIRE DimAbsence table to ONLY include Sickness records
						    #"Filtered to SICK Group Only" = Table.SelectRows(#"Added Sickness Reporting Category", each ([AbsenceGroup] = "SICK")),
						    #"Filtered Rows" = Table.SelectRows(#"Filtered to SICK Group Only", each ([Sickness Reporting Category] = "Sickness - Long-term (>20 days)" or [Sickness Reporting Category] = "Sickness - Medium-term (Medical Intervention)" or [Sickness Reporting Category] = "Sickness - Short-term"))
						in
						    #"Filtered Rows"
						```

			annotation PBI_ResultType = Table

			annotation PBI_NavigationStepName = Navigation

