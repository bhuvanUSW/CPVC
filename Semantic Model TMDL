				annotation PBI_FormatHint = {"isGeneralNumber":true}

			measure 'MoM Headcount Change %' = ```
					DIVIDE(
						SUM(FactWorkForceMonthly[FTEEOPHeadcount]) -
						CALCULATE(
							SUM(FactWorkForceMonthly[FTEEOPHeadcount]),
							DATEADD(DimDate[FullDate], -1, MONTH)
						),
						CALCULATE(
							SUM(FactWorkForceMonthly[FTEEOPHeadcount]),
							DATEADD(DimDate[FullDate], -1, MONTH)
						)
					)
					
					```
				lineageTag: 4d64d464-7a25-43dc-ad02-f4aa204267fd

				annotation PBI_FormatHint = {"isGeneralNumber":true}

			measure 'Bradford Score' = ```
					VAR AbsenceOccurrences = SUM(FactAbsenceMonthly[FTEAbsenceOccurrences])
					VAR TotalDaysLost = SUM(FactAbsenceMonthly[FTEAbsenceDays])
					RETURN
						POWER(AbsenceOccurrences, 2) * TotalDaysLost
					
					```
				lineageTag: 9a321bc7-e852-48e7-95d4-be73c7a1e2f5

				annotation PBI_FormatHint = {"isGeneralNumber":true}

			measure 'Rolling Turnover Rate' = ```
					DIVIDE(
						CALCULATE(
							SUM(FactWorkForceMonthly[FTELeavers]),
							DATESINPERIOD(
								DimDate[FullDate],
								LASTDATE(DimDate[FullDate]),
								-12,
								MONTH
							)
						),
						CALCULATE(
							AVERAGE(FactWorkForceMonthly[FTEEOPHeadcount]),
							DATESINPERIOD(
								DimDate[FullDate],
								LASTDATE(DimDate[FullDate]),
								-12,
								MONTH
							)
						)
					)
					
					```
				lineageTag: f9e87c25-3d47-4e8b-b129-d3a4f7c2e8a1

				annotation PBI_FormatHint = {"isGeneralNumber":true}

