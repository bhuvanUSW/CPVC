			measure 'Employees with Bradford Score > 192' =
					
					CALCULATE(
					    DISTINCTCOUNT(DimPerson[PersonKey]),
					    FILTER(
					        DimPerson, // Or a summarized table of PersonKey and their Bradford Score
					        [Bradford Score] > 192
					    )
					)
				formatString: 0
				lineageTag: 5f553e9c-b53a-4bd2-95e6-ea3af3ae5063

			measure 'Average Sickness Days per Employee' =
					
					VAR TotalSicknessDays = [Total Sickness Days Lost]
					VAR HeadcountForAvg = [Total Headcount EOP (Excl. HPL)] // This is SUM(FactWorkForceMonthly[FTEEOPHeadcount])
					RETURN
					IF(
					    ISBLANK(HeadcountForAvg) || HeadcountForAvg = 0,
					    BLANK(),
					    DIVIDE(TotalSicknessDays, HeadcountForAvg)
					)
				lineageTag: 72ef64dc-4747-4725-a771-d8ae110da829

				annotation PBI_FormatHint = {"isGeneralNumber":true}

			measure 'Count of Colleagues with Bradford Score > 192' =
					
					COUNTROWS(
					    FILTER(
					        // Create a temporary table of unique people (PersonKey) who have absence data
					        // and calculate their Bradford Score in the current filter context.
					        SUMMARIZE(
					            FactAbsenceMonthly, // Your fact table for absences
					            DimPerson[PersonKey],    // Group by each person
					            "@CurrentBradfordScore", [Bradford Score] // Calculate Bradford Score for each person
					        ),
					        // Now filter this temporary table to keep only those whose score is > 192
					        [@CurrentBradfordScore] > 192
					    )
					)
				formatString: 0
				lineageTag: 6193abbf-c70c-4a36-a198-600cae622776

