			partition FactAbsenceMonthly = m
				mode: import
				source =
						// M Code for: FactAbsenceMonthly (Filtered to SICKNESS ONLY for CPVC Dashboard)
						let
						    // Step 1: Source Connection and Table Access
						    Source_DB = Sql.Database("mhradb1.database.windows.net", "mhra-platform-UNSOW"),
						    FactAbsenceMonthly_SourceTable = Source_DB{[Schema="Reporting", Item="FactAbsenceMonthly"]}[Data],
						
						    // Step 2: Define Date Threshold for Filtering
						    StartDateThreshold_Date = #date(2024, 1, 1),
						    StartDateKey_Int = Date.Year(StartDateThreshold_Date) * 10000 + Date.Month(StartDateThreshold_Date) * 100 + Date.Day(StartDateThreshold_Date),
						    #"Filtered by DateKey" = Table.SelectRows(FactAbsenceMonthly_SourceTable, each [FirstDayOfMonthKey] >= StartDateKey_Int),
						
						    // Step 3: Select Necessary Columns (ensure AbsenceKey is here for the join)
						    #"Selected Columns" = Table.SelectColumns(#"Filtered by DateKey", {
						        "AbsenceMonthlyKey", "FirstDayOfMonthKey", "PositionKey", "PersonKey", "UnitStructureKey",
						        "AbsenceKey", // Needed for join to DimAbsence
						        "OrganisationKey",
						        "FTEAbsenceDays", "FTEAbsenceOccurrences",
						        "FTEUnscheduledAbsenceDays", "FTEUnscheduledAbsenceOccurrences"
						        // Add other measures if needed for SICKNESS analysis only
						    }),
						
						    // Step 4: Change Data Types
						    #"Changed Column Types" = Table.TransformColumnTypes(#"Selected Columns",{
						        {"AbsenceMonthlyKey", Int64.Type}, {"FirstDayOfMonthKey", Int64.Type},
						        {"PositionKey", Int64.Type}, {"PersonKey", Int64.Type},
						        {"UnitStructureKey", Int64.Type}, {"AbsenceKey", Int64.Type},
						        {"OrganisationKey", Int64.Type},
						        {"FTEAbsenceDays", type number}, {"FTEAbsenceOccurrences", type number},
						        {"FTEUnscheduledAbsenceDays", type number}, {"FTEUnscheduledAbsenceOccurrences", type number}
						    }),
						
						    // Step 5: Inner Join with the SICK-only DimAbsence table to keep only sickness facts
						    // This assumes your DimAbsence query is already updated and named "DimAbsence"
						    // and that it only contains SICK records and has the "AbsenceKey"
						    #"Joined with SICK DimAbsence" = Table.NestedJoin(
						        #"Changed Column Types", {"AbsenceKey"}, // Key from this FactAbsenceMonthly table
						        DimAbsence,            {"AbsenceKey"}, // Key from your SICK-only DimAbsence table
						        "JoinedDimAbsence",
						        JoinKind.Inner         // Inner join keeps only matching (i.e., SICK) records
						    ),
						
						    // Step 6: Remove the joined table column as we only needed it for filtering
						    #"Removed Joined DimAbsence Column" = Table.RemoveColumns(#"Joined with SICK DimAbsence", {"JoinedDimAbsence"})
						in
						    #"Removed Joined DimAbsence Column"

			annotation PBI_ResultType = Table

			annotation PBI_NavigationStepName = Navigation

